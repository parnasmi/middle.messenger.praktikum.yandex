{
  "name": "chat-messenger",
  "version": "1.0.0",
  "description": "Chat Messenger",
  "scripts": {
    "dev": "npm run clean && parcel static/index.html",
    "build": "webpack --mode=production --node-env=production",
    "server": "node app.js",
    "start": "npm run clean && parcel build static/index.html && node app.js",
    "clean": "rimraf ./dist",
    "eslint": "eslint 'src/**/*.ts'",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --require ts-node/register src/**/*.spec.ts",
    "test-b": "./node_modules/.bin/mocha",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parnasmi/middle.messenger.praktikum.yandex.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/parnasmi/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/parnasmi/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@webpack-cli/generators": "^2.4.2",
    "autoprefixer": "^10.4.2",
    "chai": "^4.3.6",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "express": "^4.17.3",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^19.0.0",
    "mocha": "^9.2.2",
    "postcss": "^8.4.8",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@types/hbs": "^4.0.1",
    "nanoid": "^3.3.1"
  }
}
